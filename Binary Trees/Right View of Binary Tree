void solve(Node *root,int *maxlevel,int level,vector<int>&v)
{
    if(root==NULL)
    {
        return;
    }
    if(*maxlevel<level)
    {
        v.push_back(root->data);
        *maxlevel=level;
    }
    solve(root->right,maxlevel,level+1,v);
    solve(root->left,maxlevel,level+1,v);
}
vector<int> rightView(Node *root)
{
    vector<int>v;
    int maxlevel=0;
    solve(root,&maxlevel,1,v);
    return v;
   // Your Code here
}
