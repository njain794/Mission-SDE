void pre(node *root,map<int,vector<int>>&m,int d)
{
    if(root==NULL)
    {
        return;
    }
    m[d].push_back(root->data);
    pre(root->left,m,d+1);
    pre(root->right,m,d);
    return;
}
vector<int> diagnol(node *root)
{
    map<int,vector<int>>m;
    int d=0;
    pre(root,m,d);
    vector<int>v;
    for(auto x:m)
    {
        for(auto i=0;i<x.second.size();i++)
        {
            v.push_back(x.second[i]);
        }
    }
    return v;
}
